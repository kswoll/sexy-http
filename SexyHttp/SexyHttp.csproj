<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>2.0.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>kirk</Authors>
    <PackageProjectUrl>https://github.com/kswoll/sexy-http</PackageProjectUrl>
    <Description>Http client library</Description>
    <Copyright>Copyright 2018</Copyright>
    <PackageLicenseUrl>http://www.opensource.org/licenses/mit-license.php</PackageLicenseUrl>
    <PackageTags>sexy-http http client proxy</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="SexyProxy" Version="2.0.104" />
  </ItemGroup>

</Project>
